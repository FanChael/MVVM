<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerVP2">
        <attr name="orientation">
            <!-- Defines an horizontal widget. -->
            <enum name="horizontal" value="0" />
            <!-- Defines a vertical widget. -->
            <enum name="vertical" value="1" />
        </attr>
        <attr name="stokeSize" format="dimension" />
        <attr name="cornerColor" format="color" />
        <attr name="dotRadius" format="dimension" />
        <attr name="dotColor" format="color" />
        <attr name="dotSelectColor" format="color" />
        <attr name="dotSpace" format="dimension" />
        <attr name="indicatorType">
            <enum name="DOT_LEFT" value="1" />
            <enum name="DOT_CENTER" value="2" />
            <enum name="DOT_RIGHT" value="3" />
        </attr>
    </declare-styleable>
</resources>